{
  "name": "meditor-one",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build",
    "lint": "eslint --ext .js,.vue --fix . && stylelint --fix \"{src,tests/e2e/demo}/**/*.{css,vue}\" && markdownlint docs/*.md *.md && prettier --write --loglevel warn \"**/*.{js,json,css,vue,md}\"",
    "test:unit": "vue-cli-service test:unit --silent",
    "test:e2e": "vue-cli-service test:e2e",
    "test-watch": "vue-cli-service test:unit --watch",
    "build:grammar": "nearleyc src/utils/snippet-tree/grammar.ne -o src/utils/snippet-tree/grammar.ts",
    "commit": "npx git-cz"
  },
  "dependencies": {
    "@types/lodash": "4.14.119",
    "ajv": "6.6.1",
    "firebase": "5.6.0",
    "lodash-es": "4.17.11",
    "mathjs": "5.3.1",
    "monaco-editor": "0.15.6",
    "moo": "^0.5.0",
    "nearley": "^2.15.1",
    "register-service-worker": "1.5.2",
    "slug": "^0.9.2",
    "uniqid": "^5.0.3",
    "vue": "2.5.17",
    "vue-class-component": "^6.0.0",
    "vue-multipane": "0.9.5",
    "vue-property-decorator": "7.2.0",
    "vue-router": "^3.0.1",
    "vue-wait": "^1.3.2",
    "vuetify": "1.3.12",
    "vuex": "3.0.1",
    "vuexfire": "2.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.1.2",
    "@cypress/webpack-preprocessor": "^4.0.2",
    "@types/jest": "23.3.10",
    "@types/lodash-es": "^4.17.1",
    "@types/slug": "^0.9.0",
    "@vue/cli-plugin-babel": "3.2.0",
    "@vue/cli-plugin-e2e-cypress": "3.2.0",
    "@vue/cli-plugin-eslint": "3.2.1",
    "@vue/cli-plugin-pwa": "3.2.0",
    "@vue/cli-plugin-typescript": "3.2.0",
    "@vue/cli-plugin-unit-jest": "3.2.0",
    "@vue/cli-service": "3.2.0",
    "@vue/eslint-config-prettier": "4.0.1",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/eslint-config-typescript": "3.2.0",
    "@vue/test-utils": "1.0.0-beta.26",
    "babel-core": "7.0.0-bridge.0",
    "commitizen": "3.0.5",
    "cz-conventional-changelog": "^2.1.0",
    "cz-customizable": "^5.3.0",
    "eslint-plugin-vue": "^5.0.0-beta.5",
    "eslint": "^5.9.0",
    "husky": "^1.2.0",
    "imagemin-lint-staged": "^0.3.0",
    "lint-staged": "8.1.0",
    "markdownlint-cli": "^0.13.0",
    "monaco-editor-webpack-plugin": "1.7.0",
    "node-sass": "4.10.0",
    "prettier": "^1.15.3",
    "sass-loader": "7.1.0",
    "stylelint-config-css-modules": "^1.3.0",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-recess-order": "^2.0.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint": "^9.7.1",
    "ts-jest": "23.10.5",
    "typescript": "^3.1.6",
    "vue-cli-plugin-vuetify": "^0.4.6",
    "vue-cli-plugin-webpack-bundle-analyzer": "^1.2.0",
    "vue-template-compiler": "^2.5.16",
    "webpack": "4.26.1"
  },
  "main": "src/main.ts",
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./cz-config.js"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{*.json,.*rc}": [
      "prettier --write --loglevel warn --parser json"
    ],
    "*.js": [
      "eslint --fix",
      "prettier --write --loglevel warn",
      "git add"
    ],
    "*.ts": [
      "eslint --fix",
      "prettier --write --loglevel warn",
      "git add"
    ],
    "*.vue": [
      "eslint --fix",
      "stylelint --fix",
      "prettier --write --loglevel warn",
      "git add"
    ],
    "*.css": [
      "stylelint --fix",
      "prettier --write --loglevel warn",
      "git add"
    ],
    "*.md": [
      "markdownlint",
      "prettier --write --loglevel warn",
      "git add"
    ],
    "*.{png,jpeg,jpg,gif,svg}": [
      "imagemin-lint-staged",
      "git add"
    ]
  }
}
