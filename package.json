{
  "name": "meditor-one",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build --modern",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "test-watch": "vue-cli-service test:unit --watch",
    "build:grammar": "nearleyc src/utils/snippet-tree/grammar.ne -o src/utils/snippet-tree/grammar.ts",
    "commit": "npx git-cz",
    "commitmsg": "commitlint -E GIT_PARAMS"
  },
  "dependencies": {
    "@types/lodash": "^4.14.116",
    "firebase": "^5.4.0",
    "lodash": "^4.17.10",
    "mathjs": "^4.4.2",
    "monaco-editor": "^0.14.2",
    "moo": "git+https://github.com/no-context/moo.git",
    "nearley": "^2.15.1",
    "register-service-worker": "^1.5.2",
    "uniqid": "^5.0.3",
    "vue": "^2.5.17",
    "vue-class-component": "^6.2.0",
    "vue-multipane": "^0.9.5",
    "vue-property-decorator": "^6.1.0",
    "vue-router": "^3.0.1",
    "vuetify": "^1.0.18",
    "vuex": "^3.0.1",
    "vuexfire": "^2.3.0"
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "@vue/cli-plugin-babel": "^3.0.1",
    "@vue/cli-plugin-e2e-cypress": "^3.0.1",
    "@vue/cli-plugin-eslint": "^3.0.1",
    "@vue/cli-plugin-pwa": "^3.0.1",
    "@vue/cli-plugin-typescript": "^3.0.1",
    "@vue/cli-plugin-unit-jest": "^3.0.1",
    "@vue/cli-service": "^3.0.1",
    "@vue/eslint-config-prettier": "^3.0.1",
    "@vue/eslint-config-typescript": "^3.0.1",
    "@vue/test-utils": "^1.0.0-beta.24",
    "@commitlint/cli": "7.2.1",
    "@commitlint/config-conventional": "7.1.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-plugin-dynamic-import-node": "^2.1.0",
    "commitizen": "3.0.4",
    "cz-conventional-changelog": "2.1.0",
    "cz-customizable": "5.2.0",
    "husky": "1.1.2",
    "lint-staged": "7.3.0",
    "monaco-editor-webpack-plugin": "^1.5.1",
    "node-env-run": "3.0.2",
    "node-sass": "^4.9.3",
    "sass-loader": "^7.0.3",
    "ts-jest": "^23.1.4",
    "typescript": "^3.0.1",
    "vue-cli-plugin-vuetify": "^0.1.4",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.16.5"
  },
  "main": "src/main.ts",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npx vue-cli-service lint",
      "git add"
    ],
    "*.ts": [
      "npx vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "npx vue-cli-service lint",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./cz-config.js"
    }
  }
}
